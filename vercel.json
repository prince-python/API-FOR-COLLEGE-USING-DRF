{
    "version": 2,
    "builds": [
    {
    "src": "api/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
    "src": "build_files.sh"
    }
    ],
    "routes": [
    {
    "src": "/(.*)",
    "dest": "api/wsgi.py"
    }
    ]
    }
